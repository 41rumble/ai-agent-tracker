# Server Configuration
PORT=3000
NODE_ENV=development

# MongoDB Connection
MONGODB_URI=mongodb://localhost:27017/ai-agent-tracker

# OpenAI API (used for both AI assistance and web search)
OPENAI_API_KEY=your_openai_api_key

# Authentication
JWT_SECRET=your_jwt_secret
JWT_EXPIRY=24h

# CORS Settings
# Comma-separated list of allowed origins (only used in production)
# In development, all origins are allowed by default
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080,http://192.168.1.100:3000